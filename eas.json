{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "NODE_ENV": "development",
        "API_BASE_URL": "http://10.0.2.2:8000/oneFourSeven/",
        "DEBUG_LOGGING": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_API_URL": "https://YOUR_RAILWAY_APP_NAME.up.railway.app/oneFourSeven/",
        "DEBUG_LOGGING": "false"
      }
    },
    "beta": {
      "distribution": "internal",
      "channel": "beta",
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_API_URL": "https://snookerapp.up.railway.app/oneFourSeven/",
        "DEBUG_LOGGING": "false"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "NODE_ENV": "production",
        "API_BASE_URL": "https://snookerapp-server-production.up.railway.app/oneFourSeven/",
        "DEBUG_LOGGING": "false",
        "ENABLE_SSL_PINNING": "true"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production-aab": {
      "extends": "production",
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "avielpahima@gmail.com",
        "ascAppId": "TO_BE_SET_AFTER_APP_STORE_CONNECT_SETUP",
        "appleTeamId": "TO_BE_SET_AFTER_APPLE_DEVELOPER_ENROLLMENT"
      },
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "internal"
      }
    }
  }
}
