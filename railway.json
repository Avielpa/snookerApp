{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "nixpacks",
    "buildCommand": "cd maxBreak && pip install -r requirements.txt"
  },
  "deploy": {
    "startCommand": "cd maxBreak && python manage.py runserver 0.0.0.0:$PORT",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "always"
  }
}