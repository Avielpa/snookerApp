# MaxBreak Snooker App - Development Environment Configuration

# Node environment
NODE_ENV=development

# Backend API Base URL - Using Railway production by default for maximum compatibility
# For emulator development with local Django server, use: http://10.0.2.2:8000/oneFourSeven/
# For physical device development with local Django server, use: http://YOUR_IP:8000/oneFourSeven/
EXPO_PUBLIC_API_BASE_URL=https://snookerapp.up.railway.app/oneFourSeven/

# API timeout in milliseconds
API_TIMEOUT=10000

# Enable debug logging
DEBUG_LOGGING=true

# JWT token storage keys
JWT_TOKEN_KEY=userToken
JWT_REFRESH_TOKEN_KEY=refreshToken

# Feature flags for development
ENABLE_LIVE_UPDATES=true
ENABLE_PUSH_NOTIFICATIONS=false
ENABLE_ANALYTICS=false
ENABLE_NETWORK_INSPECTOR=true

# Build configuration
BUILD_VARIANT=development
APP_VERSION=1.0.0